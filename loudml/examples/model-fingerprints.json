{
  "timestamp_field": "@timestamp",
  "interval": "1m",
  "type": "fingerprints",
  "span": "7d",
  "key": "account_ref",
  "max_keys": 20000,
  "width": 50,
  "height": 50,
  "bucket_interval": "6h",
  "offset": "30s",
  "aggregations": [
    {
      "measurement": "xdr",
      "features": [
        {
          "field": "duration",
          "name": "count-all",
          "metric": "count"
        },
        {
          "field": "duration",
          "name": "mean-all-duration",
          "metric": "avg"
        },
        {
          "field": "duration",
          "name": "std-all-duration",
          "metric": "stddev"
        }
      ]
    },
    {
      "measurement": "xdr",
      "match_all": [{"tag": "international", "value": true}],
      "features": [
        {
          "field": "duration",
          "name": "count-international",
          "metric": "count"
        },
        {
          "field": "duration",
          "name": "mean-international-duration",
          "metric": "avg"
        },
        {
          "field": "duration",
          "name": "std-international-duration",
          "metric": "stddev"
        }
      ]
    },
    {
      "measurement": "xdr",
      "match_all": [{"tag": "toll_call", "value": true}],
      "features": [
        {
          "field": "duration",
          "name": "count-premium",
          "metric": "count"
        },
        {
          "field": "duration",
          "name": "mean-premium-duration",
          "metric": "avg"
        },
        {
          "field": "duration",
          "name": "std-premium-duration",
          "metric": "stddev"
        }
      ]
    }
  ],
  "default_datasource": "elastic",
  "name": "fraud-model"
}

