{
  "bucket_interval": "20m",
  "default_datasource": "influx",
  "features": [
    {
      "field": "foo",
      "measurement": "mydata",
      "metric": "avg",
      "name": "avg_foo"
    },
    {
      "default": 0,
      "field": "foo",
      "measurement": "mydata",
      "metric": "count",
      "name": "count_foo"
    }
  ],
  "interval": 60,
  "max_evals": 100,
  "name": "timeseries-influx",
  "offset": 30,
  "span": 5,
  "threshold": 30,
  "type": "timeseries"
}
